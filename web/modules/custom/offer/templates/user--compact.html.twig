<div class="user">
	<div class="profile">
		{% if not user.user_picture.isempty %}
			<div class="image" style="background:url('{{ file_url(user.user_picture.entity.fileuri) }}');background-repeat:no-repeat;background-size:cover;"></div>
		{% endif %}
		<div class="username">{{ user.displayname }}</div>
	</div>
</div>
